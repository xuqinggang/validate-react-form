{
    "presets": ["es2015", "stage-0", "react"],
    "plugins": ["transform-runtime", "transform-decorators-legacy"],
    "env": {
        "development": {
            'plugins': [
                [
                    "flow-runtime", {
                        "assert": true,
                        "annotate": true
                    }
                ],
            ]
        },
        "production": {
            "presets": ["flow"]
        }
    }
}
